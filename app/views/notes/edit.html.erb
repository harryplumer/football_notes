<h1>Edit Note for <%= @note.school.abbreviation %></h1>

<%= form_for [@school, @note] do |f| %>
  <div>
    <%= f.label :week %>
    <%= f.text_field :week, placeholder: "Week", required: true %>
  </div>

  <div>
    <%= f.label :body %>
    <%= f.text_area :body, placeholder: "Body", required: true %>
  </div>

  <%= f.submit %>
<% end %>

